    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>http://www.semanticweb.org/alberto.traverso/ontologies/2017/1/RADIOMICS-ontology#OWLClass_010091</IRI>
        <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#string">This filter rotates an image according to a defined rotation matrix</Literal>
